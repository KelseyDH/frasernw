<% search_data = Array.new 

Specialist.all.each do |specialist|

  entry = { "name" => specialist.name, "specialty" => specialist.specializations.collect{ |s| s.name }.to_sentence, "url" => url_for(specialist) }
  search_data << entry
  
end
-%>
pathways_search_data = <%=search_data.to_json.html_safe %>
